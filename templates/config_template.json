{
  "outputPath": "./output",
  "parseConfig": {
    "parser": "gem5_stats",
    "statsPath": "/path/to/gem5/stats",
    "statsPattern": "**/stats.txt",
    "compress": false,
    "variables": [
      {
        "name": "simTicks",
        "type": "scalar"
      },
      {
        "name": "benchmark_name",
        "type": "configuration"
      },
      {
        "name": "config_description",
        "type": "configuration"
      },
      {
        "name": "random_seed",
        "type": "configuration"
      }
    ]
  },
  "dataManagers": {
    "seedsReducer": true,
    "outlierRemover": {
      "enabled": false,
      "column": "simTicks",
      "method": "iqr",
      "threshold": 1.5
    },
    "normalizer": {
      "enabled": false,
      "baseline": {
        "config_description": "baseline_config"
      },
      "columns": ["simTicks"],
      "groupBy": ["benchmark_name"]
    }
  },
  "plots": [
    {
      "type": "bar",
      "output": {
        "filename": "execution_time",
        "format": "png",
        "dpi": 300
      },
      "data": {
        "x": "benchmark_name",
        "y": "simTicks",
        "hue": "config_description",
        "aggregate": {
          "method": "geomean",
          "groupBy": ["config_description"]
        }
      },
      "style": {
        "title": "Execution Time by Benchmark",
        "xlabel": "Benchmark",
        "ylabel": "Execution Time (normalized)",
        "width": 12,
        "height": 6,
        "legend": {
          "show": true,
          "title": "Configuration",
          "location": "best"
        },
        "ylim": [0, 1.5],
        "grid": true,
        "theme": "whitegrid"
      }
    },
    {
      "type": "heatmap",
      "output": {
        "filename": "performance_heatmap",
        "format": "png"
      },
      "data": {
        "x": "benchmark_name",
        "y": "config_description",
        "value": "simTicks"
      },
      "style": {
        "title": "Performance Heatmap",
        "width": 14,
        "height": 8,
        "theme": "default"
      }
    }
  ]
}
