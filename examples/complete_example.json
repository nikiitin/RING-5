{
  "$comment": "Complete example configuration for RING-5 Pure Python Edition",
  "outputPath": "/home/vnicolas/workspace/repos/ring-tests/example_output/",
  
  "parseConfig": {
    "parser": "gem5_stats",
    "statsPath": "/home/vnicolas/workspace/repos/ring-tests/gem5_runs/",
    "statsPattern": "**/stats.txt",
    "compress": false,
    "variables": [
      {
        "name": "simTicks",
        "type": "scalar",
        "$comment": "Total simulation ticks - execution time"
      },
      {
        "name": "system.cpu.numCycles",
        "type": "scalar",
        "rename": "cpu_cycles"
      },
      {
        "name": "system.cpu.ipc",
        "type": "scalar"
      },
      {
        "name": "system.l1d.overall_misses::total",
        "type": "scalar",
        "rename": "l1d_misses"
      },
      {
        "name": "system.l2.overall_misses::total",
        "type": "scalar",
        "rename": "l2_misses"
      },
      {
        "name": "benchmark_name",
        "type": "configuration",
        "$comment": "Benchmark identifier from directory structure"
      },
      {
        "name": "config_description",
        "type": "configuration",
        "$comment": "CPU/cache configuration description"
      },
      {
        "name": "random_seed",
        "type": "configuration",
        "$comment": "Random seed for this run"
      }
    ]
  },
  
  "dataManagers": {
    "$comment": "Data preprocessing operations applied in order",
    
    "seedsReducer": true,
    "$comment": "Combine multiple random seed runs by calculating mean and std dev",
    
    "outlierRemover": {
      "enabled": true,
      "column": "simTicks",
      "method": "iqr",
      "threshold": 1.5,
      "$comment": "Remove outliers using IQR method (Q1-1.5*IQR, Q3+1.5*IQR)"
    },
    
    "normalizer": {
      "enabled": true,
      "baseline": {
        "config_description": "baseline_o3_2ghz"
      },
      "columns": ["simTicks", "cpu_cycles", "l1d_misses", "l2_misses"],
      "groupBy": ["benchmark_name"],
      "$comment": "Normalize all metrics to baseline configuration, grouped by benchmark"
    }
  },
  
  "plots": [
    {
      "$comment": "Example 1: Simple bar plot comparing execution time",
      "type": "bar",
      "output": {
        "filename": "execution_time_comparison",
        "format": "png",
        "dpi": 300
      },
      "data": {
        "x": "benchmark_name",
        "y": "simTicks",
        "hue": "config_description"
      },
      "style": {
        "title": "Execution Time by Benchmark and Configuration",
        "xlabel": "Benchmark",
        "ylabel": "Normalized Execution Time",
        "width": 14,
        "height": 8,
        "ylim": [0, 2.0],
        "grid": true,
        "theme": "whitegrid",
        "legend": {
          "show": true,
          "title": "Configuration",
          "location": "upper right"
        }
      }
    },
    
    {
      "$comment": "Example 2: Geometric mean across benchmarks",
      "type": "bar",
      "output": {
        "filename": "geomean_performance",
        "format": "png",
        "dpi": 300
      },
      "data": {
        "x": "config_description",
        "y": "simTicks",
        "aggregate": {
          "method": "geomean",
          "groupBy": ["config_description"]
        }
      },
      "style": {
        "title": "Geometric Mean Performance",
        "xlabel": "Configuration",
        "ylabel": "Normalized Performance (geomean)",
        "width": 10,
        "height": 6,
        "ylim": [0, 1.5],
        "grid": true,
        "theme": "whitegrid"
      }
    },
    
    {
      "$comment": "Example 3: Cache miss analysis with filtering",
      "type": "grouped_bar",
      "output": {
        "filename": "cache_misses",
        "format": "png",
        "dpi": 300
      },
      "data": {
        "x": "benchmark_name",
        "y": ["l1d_misses", "l2_misses"],
        "hue": "config_description",
        "filters": {
          "benchmark_name": ["bzip2", "gcc", "mcf", "hmmer"]
        }
      },
      "style": {
        "title": "L1D and L2 Cache Misses (Selected Benchmarks)",
        "xlabel": "Benchmark",
        "ylabel": "Cache Misses (normalized)",
        "width": 12,
        "height": 7,
        "grid": true,
        "theme": "whitegrid",
        "legend": {
          "show": true,
          "title": "Configuration",
          "location": "best"
        }
      }
    },
    
    {
      "$comment": "Example 4: IPC line plot showing trends",
      "type": "line",
      "output": {
        "filename": "ipc_trend",
        "format": "png",
        "dpi": 300
      },
      "data": {
        "x": "benchmark_name",
        "y": "ipc",
        "hue": "config_description"
      },
      "style": {
        "title": "Instructions Per Cycle Across Benchmarks",
        "xlabel": "Benchmark",
        "ylabel": "IPC",
        "width": 14,
        "height": 6,
        "grid": true,
        "theme": "whitegrid",
        "legend": {
          "show": true,
          "title": "Configuration"
        }
      }
    },
    
    {
      "$comment": "Example 5: Heatmap for comparing all configs and benchmarks",
      "type": "heatmap",
      "output": {
        "filename": "performance_heatmap",
        "format": "png",
        "dpi": 300
      },
      "data": {
        "x": "benchmark_name",
        "y": "config_description",
        "value": "simTicks"
      },
      "style": {
        "title": "Performance Heatmap (Normalized Execution Time)",
        "width": 16,
        "height": 10,
        "theme": "default"
      }
    },
    
    {
      "$comment": "Example 6: Box plot showing distribution across seeds",
      "type": "box",
      "output": {
        "filename": "execution_time_distribution",
        "format": "png",
        "dpi": 300
      },
      "data": {
        "x": "benchmark_name",
        "y": "simTicks",
        "hue": "config_description",
        "filters": {
          "benchmark_name": ["bzip2", "gcc", "mcf"]
        }
      },
      "style": {
        "title": "Execution Time Distribution (Before Seed Reduction)",
        "xlabel": "Benchmark",
        "ylabel": "Execution Time",
        "width": 12,
        "height": 6,
        "grid": true,
        "theme": "whitegrid",
        "legend": {
          "show": true,
          "title": "Configuration"
        }
      }
    },
    
    {
      "$comment": "Example 7: Scatter plot for correlation analysis",
      "type": "scatter",
      "output": {
        "filename": "ipc_vs_cache_misses",
        "format": "png",
        "dpi": 300
      },
      "data": {
        "x": "l1d_misses",
        "y": "ipc",
        "hue": "config_description"
      },
      "style": {
        "title": "IPC vs L1D Cache Misses",
        "xlabel": "L1D Misses (normalized)",
        "ylabel": "IPC",
        "width": 10,
        "height": 8,
        "grid": true,
        "theme": "whitegrid",
        "legend": {
          "show": true,
          "title": "Configuration"
        }
      }
    }
  ]
}
